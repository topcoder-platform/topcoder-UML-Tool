package com.topcoder.umltool.deploy.actions.undoable;

import com.topcoder.gui.diagramviewer.uml.auxiliaryelements.FreeTextNode;
import com.topcoder.umltool.deploy.MainFrame;

/**
 * <p>
 * This action is used for remove FreeTextNode from the diagram view.
 * </p>
 * @author ly, FireIce, ylouis
 * @version 1.0
 */
public class RemoveFreeTextNodeAction extends RemoveNodeAction {

    /**
     * <p>
     * Presentation name of this action.
     * </p>
     */
    private static final String PRESENTATION_NAME = "Remove FreeTextNode";

    /**
     * <p>
     * Creates an instance of RemoveFreeTextNodeAction.
     * </p>
     * @param mainFrame
     *            reference to main frame
     * @param node
     *            node to remove
     */
    public RemoveFreeTextNodeAction(MainFrame mainFrame, FreeTextNode node) {
        super(PRESENTATION_NAME, mainFrame, node, null);
    }
}
