package com.topcoder.umltool.deploy.actions.undoable;

import com.topcoder.gui.diagramviewer.uml.auxiliaryelements.PolylineNode;
import com.topcoder.umltool.deploy.MainFrame;

/**
 * <p>
 * This action is used for remove Polyline from the diagram view.
 * </p>
 * @author ly
 * @version 1.0
 */
public class RemovePolylineAction extends RemoveNodeAction {

    /**
     * <p>
     * Presentation name of this action.
     * </p>
     */
    private static final String PRESENTATION_NAME = "Remove Polyline";

    /**
     * <p>
     * Creates an instance of RemovePolylineAction.
     * </p>
     * @param mainFrame
     *            reference to main frame
     * @param node
     *            node to remove
     */
    public RemovePolylineAction(MainFrame mainFrame, PolylineNode node) {
        super(PRESENTATION_NAME, mainFrame, node, null);
    }
}
