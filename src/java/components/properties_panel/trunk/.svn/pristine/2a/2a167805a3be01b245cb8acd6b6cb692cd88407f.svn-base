<project name="Dependency Import" default="dist" basedir=".">
    <!-- property file defining the component's dependencies -->
    <property name="base_exception.jar" value="${tcs_libdir}/base_exception/2.0.0/base_exception.jar"/>
    <property name="configuration_manager.jar"
              value="${tcs_libdir}/configuration_manager/2.1.5/configuration_manager.jar"/>
    <property name="uml_model_-_core.jar" value="${tcs_libdir}/uml_model_-_core/1.0/uml_model_-_core.jar"/>
    <property name="uml_model_-_activity_graphs.jar"
              value="${tcs_libdir}/uml_model_-_activity_graphs/1.0/uml_model_-_activity_graphs.jar"/>
    <property name="uml_model_-_actions.jar" value="${tcs_libdir}/uml_model_-_actions/1.0/uml_model_-_actions.jar"/>
    <property name="uml_model_-_data_types.jar"
              value="${tcs_libdir}/uml_model_-_data_types/1.0/uml_model_-_data_types.jar"/>
    <property name="uml_model_-_core_dependencies.jar"
              value="${tcs_libdir}/uml_model_-_core_dependencies/1.0/uml_model_-_core_dependencies.jar"/>
    <property name="uml_model_-_common_behavior.jar"
              value="${tcs_libdir}/uml_model_-_common_behavior/1.0/uml_model_-_common_behavior.jar"/>
    <property name="uml_model_-_core_classifiers.jar"
              value="${tcs_libdir}/uml_model_-_core_classifiers/1.0/uml_model_-_core_classifiers.jar"/>
    <property name="uml_model_-_core_relationships.jar"
              value="${tcs_libdir}/uml_model_-_core_relationships/1.0/uml_model_-_core_relationships.jar"/>
    <property name="uml_model_-_model_management.jar"
              value="${tcs_libdir}/uml_model_-_model_management/1.0/uml_model_-_model_management.jar"/>
    <property name="uml_model_-_state_machines.jar"
              value="${tcs_libdir}/uml_model_-_state_machines/1.0/uml_model_-_state_machines.jar"/>
    <property name="uml_model_-_core_extension_mechanisms.jar"
              value="${tcs_libdir}/uml_model_-_core_extension_mechanisms/1.0/uml_model_-_core_extension_mechanisms.jar"/>
    <property name="uml_model_-_use_cases.jar"
              value="${tcs_libdir}/uml_model_-_use_cases/1.0/uml_model_-_use_cases.jar"/>
    <property name="uml_project_configuration.jar"
              value="${tcs_libdir}/uml_project_configuration/1.0.0/uml_project_configuration.jar"/>
    <property name="uml_model_manager.jar" value="${tcs_libdir}/uml_model_manager/1.0/uml_model_manager.jar"/>

    <!-- 3rd party dependencies -->
    <property name="junit.jar" value="${ext_libdir}/junit/3.8.2/junit.jar"/>
    <property name="jakarta-regexp.jar" value="${ext_libdir}/jakarta-regexp/1.5/jakarta-regexp-1.5.jar"/>
    <property name="jfcunit.jar" value="${ext_libdir}/jfcunit/2.0.8/jfcunit.jar"/>


    <path id="component.tcs-dependencies">
        <pathelement location="${base_exception.jar}"/>
        <pathelement location="${configuration_manager.jar}"/>
        <pathelement location="${uml_model_-_core.jar}"/>
        <pathelement location="${uml_model_-_actions.jar}"/>
        <pathelement location="${uml_model_-_activity_graphs.jar}"/>
        <pathelement location="${uml_model_-_data_types.jar}"/>
        <pathelement location="${uml_model_-_common_behavior.jar}"/>
        <pathelement location="${uml_model_-_core_classifiers.jar}"/>
        <pathelement location="${uml_model_-_core_dependencies.jar}"/>
        <pathelement location="${uml_model_-_core_relationships.jar}"/>
        <pathelement location="${uml_model_-_use_cases.jar}"/>
        <pathelement location="${uml_model_-_core_extension_mechanisms.jar}"/>
        <pathelement location="${uml_model_-_model_management.jar}"/>
        <pathelement location="${uml_model_-_state_machines.jar}"/>
        <pathelement location="${uml_project_configuration.jar}"/>
        <pathelement location="${uml_model_manager.jar}"/>
    </path>

    <path id="component.3rdParty-dependencies"/>

    <path id="component.test.3rdParty-dependencies">
        <pathelement location="${junit.jar}"/>
        <pathelement location="${jakarta-regexp.jar}"/>
        <pathelement location="${jfcunit.jar}"/>
    </path>

    <!-- Properties used in compile_targets ANT target -->
    <property name="component.target" value="1.5"/>
    <property name="component.bootclasspath" value="${java_1_5_bootclasspath}"/>
</project>
