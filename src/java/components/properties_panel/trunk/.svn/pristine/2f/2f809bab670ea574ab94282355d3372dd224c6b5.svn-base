package com.topcoder.gui.panels.properties;

/**
 * <p>
 * This is an enumeration which lists all the property types supported by this component.
 * </p>
 *
 * <p>
 * This enumeration provides support to determine easily whether a given property is supported
 * by a <code>PropertyPanel</code> implementation using a standard naming.
 * </p>
 *
 * <p>
 * Thread-safety: Enum is thread-safe.
 * </p>
 *
 * @author ch_music, TCSDEVELOPER
 * @version 1.0
 */
public enum PropertyKind {
    /**
     * <p>
     * Represents the name property.
     * </p>
     */
    NAME,

    /**
     * <p>
     * Represents the namespace property.
     * </p>
     */
    NAMESPACE,

    /**
     * <p>
     * Represents the owner property.
     * </p>
     */
    OWNER,

    /**
     * <p>
     * Represents the type property.
     * </p>
     */
    TYPE,

    /**
     * <p>
     * Represents the visibility property.
     * </p>
     */
    VISIBILITY,

    /**
     * <p>
     * Represents the abstract property.
     * </p>
     */
    ABSTRACT,

    /**
     * <p>
     * Represents the final property.
     * </p>
     */
    FINAL,

    /**
     * <p>
     * Represents the root property.
     * </p>
     */
    ROOT,

    /**
     * <p>
     * Represents the active property.
     * </p>
     */
    ACTIVE,

    /**
     * <p>
     * Represents the static property.
     * </p>
     */
    STATIC,

    /**
     * <p>
     * Represents the initial value property.
     * </p>
     */
    INITIAL_VALUE,

    /**
     * <p>
     * Represents the multiplicity property.
     * </p>
     */
    MULTIPLICITY,

    /**
     * <p>
     * Represents the ordering property.
     * </p>
     */
    ORDERING,

    /**
     * <p>
     * Represents the changeability property.
     * </p>
     */
    CHANGEABILITY,

    /**
     * <p>
     * Represents the transient property.
     * </p>
     */
    TRANSIENT,

    /**
     * <p>
     * Represents the concurrency property.
     * </p>
     */
    CONCURRENCY,

    /**
     * <p>
     * Represents the parameters property.
     * </p>
     */
    PARAMETERS,

    /**
     * <p>
     * Represents the kind property.
     * </p>
     */
    KIND,

    /**
     * <p>
     * Represents the aggregation property.
     * </p>
     */
    AGGREGATION,

    /**
     * <p>
     * Represents the navigable property.
     * </p>
     */
    NAVIGABLE,

    /**
     * <p>
     * Represents the outgoing transitions property.
     * </p>
     */
    OUTGOING_TRANSITIONS,

    /**
     * <p>
     * Represents the incoming transitions property.
     * </p>
     */
    INCOMING_TRANSITIONS,

    /**
     * <p>
     * Represents the guard property.
     * </p>
     */
    GUARD,

    /**
     * <p>
     * Represents the owner link property.
     * </p>
     */
    OWNER_LINK,

    /**
     * <p>
     * Represents the supplier link property.
     * </p>
     */
    SUPPLIER_LINK,

    /**
     * <p>
     * Represents the client link property.
     * </p>
     */
    CLIENT_LINK,

    /**
     * <p>
     * Represents the parent link property.
     * </p>
     */
    PARENT_LINK,

    /**
     * <p>
     * Represents the child link property.
     * </p>
     */
    CHILD_LINK,

    /**
     * <p>
     * Represents the association link property.
     * </p>
     */
    ASSOCIATION_LINK,

    /**
     * <p>
     * Represents the extension link property.
     * </p>
     */
    EXTENSION_LINK,

    /**
     * <p>
     * Represents the addition link property.
     * </p>
     */
    ADDITION_LINK,

    /**
     * <p>
     * Represents the stereotypes list property.
     * </p>
     */
    STEREOTYPES,

    /**
     * <p>
     * Represents the base link property.
     * </p>
     */
    BASE_LINK,

    /**
     * <p>
     * Represents the source link property.
     * </p>
     */
    SOURCE_LINK,

    /**
     * <p>
     * Represents the target link property.
     * </p>
     */
    TARGET_LINK,

    /**
     * <p>
     * Represents the stimulus link property.
     * </p>
     */
    STIMULUS_LINK,

    /**
     * <p>
     * Represents the association ends link property.
     * </p>
     */
    ASSOCIATION_ENDS_LINK,

    /**
     * <p>
     * Represents the asynchronous property.
     * </p>
     */
    ASYNCHRONOUS,

    /**
     * <p>
     * Represents the action link property.
     * </p>
     */
    ACTION_LINK,

    /**
     * <p>
     * Represents the namespace link property.
     * </p>
     */
    NAMESPACE_LINK,

    /*
     * BugFix: UML-9718
     * Add two kinds.
     */
    /**
     * <p>
     * Represents the operation property.
     * </p>
     */
    OPERATION,

    /**
     * <p>
     * Represents the attribute property.
     * </p>
     */
    ATTRIBUTE;
}