package com.topcoder.uml.model.core.extensionmechanisms;

import junit.framework.Test;
import junit.framework.TestCase;
import junit.framework.TestSuite;

/**
 * <p>
 * This test case aggregates all Unit test cases.
 * </p>
 *
 * @author BLE
 * @version 1.0
 */
public class UnitTests extends TestCase {

    /**
     * <p>
     * Aggregates all Unit test cases.
     * </p>
     *
     * @return the test suite
     */
    public static Test suite() {
        final TestSuite suite = new TestSuite();
        suite.addTestSuite(HelperTest.class);
        suite.addTestSuite(StereotypeImplTest.class);
        suite.addTestSuite(TagDefinitionImplTest.class);
        suite.addTestSuite(TaggedValueImplTest.class);
        suite.addTestSuite(DemoTest.class);
        return suite;
    }
}
