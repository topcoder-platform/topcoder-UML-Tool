<?xml version="1.0"?>

<!-- File containing sample configuration details for use with
 the Documentation Panel component -->
    
<CMConfig>

<!-- sample tag option manager configuration -->
    <Config name="defaultTagOptions">
    
    <Property name="TYPES">
    <Value>GLOBAL</Value>
    <Value>accuracytests.my1</Value>
    <Value>accuracytests.my2</Value>
    </Property>

        <!-- global options -->
        <Property name="GLOBAL">
            <!-- some common javadocs options -->
            <Property name="options">
                <Value>see</Value>
                <Value>since</Value>
                <Value>deprecated</Value>
            </Property>
        </Property>
        
        <!-- method options -->
        <Property name="accuracytests.my1">
            <!-- add two operation options -->
            <Property name="options">
                <Value>throws</Value>
                <Value>exception</Value>
            </Property>
            
            <!-- inherit the global set -->
            <Property name="links">
                <Value>GLOBAL</Value>
            </Property>
        </Property>
        
        <!-- class options, adding two to global -->
        <Property name="accuracytests.my2">
            <Property name="links">
                <Value>GLOBAL</Value>
            </Property>
        </Property>
    </Config>
    
    
           
</CMConfig>