<CMConfig>
    
    <Config name="defaultTagEditor_Invalid_1">
        <!-- The configuration of option manager is invalid,
             see tagOptionsInvalid.xml -->
        <Property name="tagoptionNamespace">
            <Value>tagOptions_invalid_1</Value>
        </Property>
    </Config>
    
    <Config name="defaultTagEditor_Invalid_2">
        <!-- The configuration of option manager is invalid,
             see tagOptionsInvalid.xml -->
        <Property name="tagoptionNamespace">
            <Value>tagOptions_invalid_loop_1</Value>
        </Property>
    </Config>
    
    <Config name="defaultTagEditor_Invalid_3">
        <Property name="tagprompt">
            <Value>new tag prompt</Value>
        </Property>
            
        <Property name="valueprompt">
            <Value>new Value prompt</Value>
        </Property>
        
        <!-- Size of column names must be 2:-->
        <Property name="columnnames">
            <Value>Tag doc</Value>
        </Property>
            
        <Property name="tagoptionNamespace">
            <Value>tagOptions_1</Value>
        </Property>
    </Config>

    <Config name="defaultTagEditor_Invalid_4">
        <Property name="tagprompt">
            <Value>new tag prompt</Value>
        </Property>
            
        <Property name="valueprompt">
            <Value>new Value prompt</Value>
        </Property>
        
        <!-- Size of column names must be 2:-->
        <Property name="columnnames">
            <Value>Tag name</Value>
            <Value>Tag doc</Value>
            <Value>Tag des</Value>
        </Property>
            
        <Property name="tagoptionNamespace">
            <Value>tagOptions_1</Value>
        </Property>
