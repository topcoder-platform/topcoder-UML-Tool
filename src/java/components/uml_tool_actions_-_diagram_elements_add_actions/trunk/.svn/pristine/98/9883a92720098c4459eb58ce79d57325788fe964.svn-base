package com.topcoder.uml.actions.diagram.elements.accuracytests;

import com.topcoder.diagraminterchange.GraphElement;
import com.topcoder.uml.actions.diagram.elements.DiagramElementUndoableAction;
import com.topcoder.util.actionmanager.ActionExecutionException;

/**
 * This class extends abstract class DiagramElementUndoableAction.
 * It is created for testing the other public methods in the DiagramElementUndoableAction class.
 *
 * @author Chenhong
 * @version 1.0
 */
public class MyDiagramElementUndoableAction extends DiagramElementUndoableAction {

    /**
     * Constructor.
     * @param element the GraphElement instance for this class.
     */
    protected MyDiagramElementUndoableAction(GraphElement element) {
        super(element);
    }

    /**
     * The method execute.  In this implementation, it will do nothing.
     */
    public void execute() throws ActionExecutionException {
        // does nothing.
    }


    /**
     * <p>
     * Set the last exception occurred during execution of redo or undo method.
     * </p>
     *
     * @param exception the last exception occurred during execution of redo or undo method.
     *
     * @throws IllegalArgumentException if exception is null
     */
    public void setLastException(ActionExecutionException exception) {
        super.setLastException(exception);
    }
}
