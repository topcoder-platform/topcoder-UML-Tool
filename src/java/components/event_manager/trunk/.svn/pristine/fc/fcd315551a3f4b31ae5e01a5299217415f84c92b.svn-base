package com.topcoder.util.eventmanager;

import junit.framework.Test;
import junit.framework.TestCase;
import junit.framework.TestSuite;


/**
 * <p>
 * This test case aggregates all Unit test cases.
 * </p>
 *
 * @author TopCoder
 * @version 1.0
 */
public class UnitTests extends TestCase {
    /**
     * Get the test suite.
     *
     * @return the test suite
     */
    public static Test suite() {
        final TestSuite suite = new TestSuite();
        suite.addTestSuite(ActionEventTest.class);
        suite.addTestSuite(RedoChangesEventTest.class);
        suite.addTestSuite(UndoChangesEventTest.class);
        suite.addTestSuite(EventManagerTest.class);
        suite.addTestSuite(Demo.class);
        return suite;
    }
}
