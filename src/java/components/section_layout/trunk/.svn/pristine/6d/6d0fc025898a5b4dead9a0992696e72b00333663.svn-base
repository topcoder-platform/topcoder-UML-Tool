package com.topcoder.gui.sectionlayout.accuracytests;

import com.topcoder.gui.sectionlayout.dock.Dockable;
import com.topcoder.gui.sectionlayout.dock.DragInitListener;

import java.awt.event.InputEvent;


/**
 * A simple mock listener for testing.
 *
 * @author Chenhong
 * @version 1.0
 */
class MyDragInitListener implements DragInitListener {
    /**
     * <p>
     * This method will be invoked when the listened dockable component triggered an event to start drag.
     * </p>
     *
     * @param event the input event to start drag
     * @param dockable the dockable component which is the source of the event
     */
    public void dragInitialized(InputEvent event, Dockable dockable) {
        System.out.println("Enter into method :dragInitialized");
        System.out.println(event);
    }
}
