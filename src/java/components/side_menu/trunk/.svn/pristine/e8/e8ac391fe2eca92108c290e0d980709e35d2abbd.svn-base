package com.topcoder.gui.sidemenu.accuracytests;

import java.beans.PropertyChangeEvent;

import javax.swing.JComponent;

import com.topcoder.gui.sidemenu.DefaultExpandedSideMenuPanel;
import com.topcoder.gui.sidemenu.SideMenuModel;

/**
 * This is a mock class of <code>DefaultExpandedSideMenuPanel</code>.
 *
 * @author TCSDEVELOPER
 * @version 1.0
 */
public class MyDefaultExpandedSideMenuPanel extends DefaultExpandedSideMenuPanel {
    /**
     * Simply call super(sideMenuModel, content).
     *
     * @throws IllegalArgumentException
     *             if sideMenuModel or content is null
     * @param sideMenuModel
     *            A non-null side menu model to use
     * @param content
     *            A non-null content panel to use
     */
    public MyDefaultExpandedSideMenuPanel(SideMenuModel sideMenuModel, JComponent content) {
        super(sideMenuModel, content);
    }

    /**
     * Simply call super.sideMenuModelPropertyChange(event).
     *
     * @throws IllegalArgumentException
     *             if event is null, or value of property is invalid.
     * @param event
     *            A non-null property change event
     */
    @Override
    public void sideMenuModelPropertyChange(PropertyChangeEvent event) {
        super.sideMenuModelPropertyChange(event);
    }

}
