package com.topcoder.gui.sidemenu.accuracytests;

import java.beans.PropertyChangeEvent;

import javax.swing.Icon;

import com.topcoder.gui.sidemenu.DefaultCollapsedSideMenuPanel;
import com.topcoder.gui.sidemenu.SideMenuModel;

/**
 * This is a mock class of <code>DefaultCollapsedSideMenuPanel</code>.
 *
 * @author TCSDEVELOPER
 * @version 1.0
 */
public class MyDefaultCollapsedSideMenuPanel extends DefaultCollapsedSideMenuPanel {
    /**
     * Simply call super(sideMenuModel, icon).
     *
     * @throws IllegalArgumentException
     *             if the sideMenuModel is null
     * @param sideMenuModel
     *            A non-null SideMenuModel
     * @param expandIcon
     *            A possibly null expansion icon to use
     */
    public MyDefaultCollapsedSideMenuPanel(SideMenuModel sideMenuModel, Icon expandIcon) {
        super(sideMenuModel, expandIcon);
    }

    /**
     * Simply call super.sideMenuModelPropertyChange(event).
     *
     * @throws IllegalArgumentException
     *             if event is null, value of specific property is invalid.
     * @param event
     *            A non-null property change event
     */
    @Override
    public void sideMenuModelPropertyChange(PropertyChangeEvent event) {
        super.sideMenuModelPropertyChange(event);
    }

}
