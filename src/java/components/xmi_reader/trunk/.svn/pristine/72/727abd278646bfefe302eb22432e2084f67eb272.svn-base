package com.topcoder.xmi.reader.handlers;

import com.topcoder.xmi.reader.DefaultXMIHandler;
import com.topcoder.xmi.reader.XMIReader;


/**
 * <p>
 * This is the mock class of DefaultXMIHandler. Since it only extends DefaultXMIHandler without any
 * other implementations, we call it Simple class(Note that the constructors of DefaultXMIHandler
 * are protected, so here just present two constructors which calling the corresponding super
 * constructor).
 * </p>
 *
 * @author TCSDEVELOPER
 * @version 1.0
 */
public class MockClassSimpleXMIHandler extends DefaultXMIHandler {
    /**
     * <p>
     * Creates this handler with all properties null.
     * </p>
     */
    public MockClassSimpleXMIHandler() {
        super();
    }

    /**
     * <p>
     * Constructs this handler with the given XMIReader.
     * </p>
     *
     * @param reader the XMIReader instance to attach to this handler
     *
     * @throws IllegalArgumentException if reader is null.
     */
    public MockClassSimpleXMIHandler(XMIReader reader) {
        super(reader);
    }
}
