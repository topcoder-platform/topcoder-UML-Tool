package com.topcoder.swing.birdseyeview.failuretests;

import junit.framework.Test;
import junit.framework.TestCase;
import junit.framework.TestSuite;
/**
 * Some failure tests.
 *
 * @author telly12
 * @version 1.0
 */
public class FailureTests extends TestCase {
	/**
     * <p>
     * Aggregates all unit tests.
     * </p>
     *
     * @return test suite aggregating all unit tests.
     */
    public static Test suite() {
        final TestSuite suite = new TestSuite();

        suite.addTestSuite(BirdsEyeViewFailureTests.class);
        suite.addTestSuite(ViewPortFailureTests.class);
        return suite;
    }
}
