package com.topcoder.xmi.writer.accuracytests;

import com.topcoder.xmi.writer.TransformerType;

import junit.framework.TestCase;

/**
 * <p>
 * Accuracy Test for <code>TransformerType</code>.
 * </p>
 *
 * @author restarter
 * @version 1.0
 */
public class TransformerTypeAccuracyTest extends TestCase {
    /**
     * <p>
     * Accuracy Test.
     * </p>
     */
    public void testTransformerType() {
        assertEquals("Model in TransformerType is wrong.", 0, TransformerType.Model.ordinal());
        assertEquals("ActivityGraph in TransformerType is wrong.", 1, TransformerType.ActivityGraph.ordinal());
        assertEquals("Diagram in TransformerType is wrong.", 2, TransformerType.Diagram.ordinal());
        assertEquals("Header in TransformerType is wrong.", 3, TransformerType.Header.ordinal());
    }
}
