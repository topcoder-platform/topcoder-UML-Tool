<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<shortcuts>
    <skipIfNotSupported />
    <programGroup defaultName="TopCoder UML Tool" location="applications"/>
    
    <shortcut
        name="TopCoder UML Tool"
        programGroup="yes"
        desktop="no"
        applications="no"
        startMenu="no"
        startup="no"
        target="$INSTALL_PATH/bin/run.sh"
        workingDirectory="$INSTALL_PATH/bin"
        commandLine=""
        initialState="normal"
        iconFile="$INSTALL_PATH\bin\resources\images\icons\icon.png"
        type="Application"
        encoding="UTF-8"
        description="Run the TopCoder UML Tool"
        categories="Application" >
    
        <createForPack name="Core"/>
    </shortcut>
    
    <shortcut
        name="Uninstall TopCoder UML Tool"
        programGroup="yes"
        desktop="no"
        applications="no"
        startMenu="no"
        startup="no"
        target="java"
        commandLine="-jar &quot;$INSTALL_PATH/Uninstaller/uninstaller.jar&quot;"
        workingDirectory=""
        initialState="normal"
        iconFile="$INSTALL_PATH\bin\resources\images\icons\icon.png"
        encoding="UTF-8"
        type="Application"
        TryExec="java -jar &quot;$INSTALL_PATH/Uninstaller/uninstaller.jar&quot;"
        description="Uninstall the TopCoder UML Tool"
        categories="Application" >
        <createForPack name="Core"/>
    </shortcut>

</shortcuts>

